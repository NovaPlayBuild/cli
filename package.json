{
  "name": "@hyperplay/cli",
  "version": "2.13.0",
  "description": "Hyperplay CLI",
  "author": "NovaPlay Labs, Inc.",
  "bin": {
    "hyperplay": "./bin/run"
  },
  "homepage": "https://github.com/hyperplay/cli",
  "license": "MPL-2.0",
  "main": "dist/index.js",
  "repository": "hyperplay/cli",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/package-lock.json"
  ],
  "dependencies": {
    "@oclif/core": "^1.8.0",
    "@oclif/plugin-help": "^5",
    "@valist/sdk": "^2.9.13",
    "archiver": "^7.0.0",
    "axios": "^1.6.7",
    "axios-cookiejar-support": "^5.0.0",
    "ethers": "^6.0.8",
    "files-from-path": "1.0.0",
    "inquirer": "^8.2.4",
    "keytar": "^7.9.0",
    "qs": "^6.12.0",
    "siwe": "^2.1.4",
    "tough-cookie": "^4.1.3",
    "typescript-plugin-css-modules": "5.0.1",
    "ua-parser-js": "1.0.35",
    "yaml": "^2.1.0"
  },
  "devDependencies": {
    "@types/archiver": "^6.0.2",
    "@types/chai": "^4",
    "@types/inquirer": "^8.2.1",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.4",
    "@types/qs": "^6.9.12",
    "@types/tough-cookie": "^4.0.5",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "chai": "^4",
    "eslint": "^8.15.0",
    "eslint-plugin-mocha": "^10.0.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-unicorn": "^42.0.0",
    "globby": "^11",
    "hardhat": "^2.22.3",
    "mocha": "^9",
    "nock": "^13.5.4",
    "nyc": "^15.1.0",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.6.4"
  },
  "oclif": {
    "bin": "hyperplay",
    "dirname": "hyperplay",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " "
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b && oclif manifest && oclif readme",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "test": "mocha -r ts-node/register 'test/**/*.test.ts' --timeout 120000",
    "version": "oclif readme && git add README.md",
    "startLocalHardhat": "hardhat node",
    "coverage": "nyc npm run test"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/hyperplay/cli/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}